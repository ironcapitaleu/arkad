# .env
RABBITMQ_DEFAULT_USER=admin # Default user for RabbitMQ, needs to be in sync with definitions.json file
RABBITMQ_DEFAULT_PASS=admin123 # Default password for RabbitMQ, needs to be in sync with definitions.json file
EXTRACTION_RESULTS_QUEUE=batch.extraction.results
TRANSFORMATION_RESULTS_QUEUE=batch.transformation.results
RABBITMQ_HOST=localhost # Hostname for RabbitMQ, default is localhost (okay for local development)
RABBITMQ_PORT=5672 # Default port. Matches the port in compose.yaml
RABBITMQ_VHOST=/
